<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="heven.holt.model.camera">

    <application
            android:name="debug.CameraApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/camera_app_name"
            android:supportsRtl="true"
            android:theme="@style/common_AppTheme">
        <activity android:name="debug.CameraActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="heven.holt.model.camera.ui.SurfaceViewCreateActivity"/>
        <activity
                android:name="heven.holt.model.camera.ui.SurfaceViewCameraActivity"/>
    </application>
</manifest>

